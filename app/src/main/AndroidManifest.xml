<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.groceryapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".AdminPage"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".AddProduct"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".AddCoupon"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ListUpdatePrice"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ListCoupons"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".DeleteCoupons"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".UserPage"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ViewShopList"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>